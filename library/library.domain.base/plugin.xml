<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            genModel="src/library/base/Library.xcoreiq"
            uri="library.base"
            class="library.base.BasePackage">
      </package>
   </extension>
   <extension
         id="extension.derived.name"
         point="org.eclipse.incquery.runtime.base.wellbehaving.derived.features">
      <wellbehaving-derived-feature
            classifier-name="Writer"
            feature-name="name"
            package-nsUri="library.base">
      </wellbehaving-derived-feature>
   </extension>
   <extension
         id="library.base.writerName"
         point="org.eclipse.incquery.runtime.queryspecification">
      <matcher
            querySpecificationProvider="library.base.util.WriterNameQuerySpecification$Provider"
            id="library.base.writerName">
      </matcher>
   </extension>

</plugin>
